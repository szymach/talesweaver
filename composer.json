{
    "name": "szymach/talesweaver",
    "description": "Book writing app",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Piotr Szymaszek",
            "email": "pszymaszek@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Domain\\": "src/Domain/"
        },
        "files": ["src/App/Security/function/generate_user_token.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": [
                "tests/functional/App",
                "tests/unit/App"
            ]
        }
    },
    "require": {
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "egeloen/ckeditor-bundle": "^6.0",
        "ext-exif": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "fsi/doctrine-extensions": "^2.0@dev",
        "fsi/doctrine-extensions-bundle": "^2.0@dev",
        "jms/i18n-routing-bundle": "^2.0",
        "knplabs/knp-menu-bundle": "^2.1",
        "knplabs/knp-snappy-bundle": "^1.4",
        "php": ">=7.2",
        "ramsey/uuid-doctrine": "^1.2",
        "simple-bus/symfony-bridge": "^5.1",
        "symfony/asset": "^4.0",
        "symfony/console": "^4.0",
        "symfony/expression-language": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^3.4",
        "symfony/framework-bundle": "^3.4",
        "symfony/monolog-bundle": "^3.1",
        "symfony/polyfill-apcu": "^1.5",
        "symfony/security-bundle": "^4.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/yaml": "^4.0",
        "twig/extensions": "^1.5",
        "white-october/pagerfanta-bundle": "^1.0"
    },
    "require-dev": {
        "codeception/codeception": "^2.3",
        "codeception/c3": "^2.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/browser-kit": "^4.0",
        "symfony/css-selector": "^4.0",
        "symfony/debug-bundle": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/web-profiler-bundle": "^4.0",
        "symfony/web-server-bundle": "^4.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "ckeditor:install -n --clear=skip public/bundles/ivoryckeditor": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "id": "01C2MKG8BFE7N5Z4GNA7AHV5MM",
            "allow-contrib": true
        }
    }
}
