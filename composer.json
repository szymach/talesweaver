{
    "name": "szymach/talesweaver",
    "description": "Book writing app",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Piotr Szymaszek",
            "email": "pszymaszek@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Talesweaver\\Application\\": "src/Application/",
            "Talesweaver\\Doctrine\\": "src/Doctrine/",
            "Talesweaver\\Domain\\": "src/Domain/",
            "Talesweaver\\Integration\\": "src/Integration/"
        },
        "files": ["src/Integration/Security/function/generate_user_token.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Talesweaver\\Tests\\Domain\\": ["tests/unit/Domain"],
            "Talesweaver\\Tests\\Integration\\": [
                "tests/functional/Integration",
                "tests/unit/Integration"
            ],
            "Talesweaver\\Tests\\Helper\\": ["tests/_support/Helper"]
        }
    },
    "require": {
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.9",
        "ext-exif": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "fsi/doctrine-extensions-bundle": "^2.0",
        "knplabs/knp-menu-bundle": "^2.1",
        "knplabs/knp-snappy-bundle": "^1.4",
        "php": ">=7.2",
        "ramsey/uuid-doctrine": "^1.2",
        "simple-bus/symfony-bridge": "^5.1",
        "symfony/asset": "^4.1",
        "symfony/console": "^4.1",
        "symfony/expression-language": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/monolog-bundle": "^3.1",
        "symfony/polyfill-apcu": "^1.5",
        "symfony/security-bundle": "^4.1",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^4.1",
        "symfony/templating": "^4.1",
        "symfony/twig-bundle": "^4.1",
        "symfony/validator": "^4.1",
        "symfony/yaml": "^4.1",
        "twig/extensions": "^1.5",
        "white-october/pagerfanta-bundle": "^1.0"
    },
    "require-dev": {
        "codeception/codeception": "^2.3",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/browser-kit": "^4.1",
        "symfony/css-selector": "^4.1",
        "symfony/debug-bundle": "^4.1",
        "symfony/dotenv": "^4.1",
        "symfony/phpunit-bridge": "^4.1",
        "symfony/web-profiler-bundle": "^4.1",
        "symfony/web-server-bundle": "^4.1",
        "vlucas/phpdotenv": "^2.4"
    },
    "config": {
        "bin-dir": "vendor/bin",
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "id": "01C2MKG8BFE7N5Z4GNA7AHV5MM",
            "allow-contrib": true
        }
    }
}
